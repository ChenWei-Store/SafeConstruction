<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.shuangning.safeconstruction.base.widget.TitleView
        android:id="@+id/view_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_title"
        android:layout_marginStart="@dimen/horizontal_margin_24"
        android:layout_marginEnd="@dimen/horizontal_margin_24"
        android:layout_marginTop="@dimen/vertical_margin_24"
        android:background="@drawable/common_8_0a8de5_bg"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/view1"
        app:layout_constraintEnd_toEndOf="@id/view1"
        app:layout_constraintTop_toTopOf="@id/view1"
        app:layout_constraintBottom_toBottomOf="@id/view1"
        android:gravity="center_vertical"
        android:includeFontPadding="false"
        android:drawableLeft="@drawable/common_person_bg"
        android:drawablePadding="@dimen/horizontal_margin_8"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/c_fff"
        android:text="项目主要人员考勤数据"/>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view1"
        android:layout_marginStart="@dimen/horizontal_margin_24"
        android:layout_marginEnd="@dimen/horizontal_margin_24"
        android:layout_marginTop="@dimen/vertical_margin_24"
        android:background="@drawable/common_8_00cdcc_bg"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintStart_toStartOf="@id/view2"
        app:layout_constraintEnd_toEndOf="@id/view2"
        app:layout_constraintTop_toTopOf="@id/view2"
        app:layout_constraintBottom_toBottomOf="@id/view2"
        android:gravity="center_vertical"
        android:includeFontPadding="false"
        android:drawableLeft="@drawable/manage_person_bg"
        android:drawablePadding="@dimen/horizontal_margin_8"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/c_fff"
        android:text="安全管理人员考勤数据"/>
</androidx.constraintlayout.widget.ConstraintLayout>