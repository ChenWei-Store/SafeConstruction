            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tv_rectification_requirement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="整改要求"
            app:layout_constraintTop_toBottomOf="@id/view_line9"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            android:layout_marginTop="@dimen/vertical_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555" />

        <TextView
            android:id="@+id/tv_rectification_requirement_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
