            android:layout_marginTop="@dimen/vertical_margin_8"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="整改期限"/>

        <TextView
            android:id="@+id/tv_rectification_period"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/tv_rectification_period_title"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_ff3e3d"
            android:text="2022-12-31 00：00"/>
        <View
            android:id="@+id/view_line11"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/divider"
            android:layout_marginTop="@dimen/vertical_margin_8"
