            app:layout_constraintTop_toTopOf="@id/tv_rectification_requirement"
            app:layout_constraintBottom_toBottomOf="@id/tv_rectification_requirement"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"/>

        <View
            android:id="@+id/view_line10"
            android:layout_width="match_parent"
            android:layout_height="1dp"
