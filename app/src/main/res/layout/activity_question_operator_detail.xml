<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/c_fff">
        <com.shuangning.safeconstruction.base.widget.TitleView
            android:id="@+id/view_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/tv_problem_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/view_title"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/vertical_margin_8"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="测试测试测试测试"/>
        <ImageView
            android:id="@+id/iv"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginTop="@dimen/vertical_margin_8"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            app:layout_constraintTop_toBottomOf="@id/tv_problem_content"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/test_round_bg"/>
        <View
            android:id="@+id/view_line2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/divider"
            android:layout_marginTop="@dimen/vertical_margin_8"
            app:layout_constraintTop_toBottomOf="@id/iv"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tv_content_number_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/view_line2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/vertical_margin_8"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="内容编号"/>

        <TextView
            android:id="@+id/tv_content_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/tv_content_number_title"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="GX-2_221228001"/>
        <View
            android:id="@+id/view_line3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/divider"
            android:layout_marginTop="@dimen/vertical_margin_8"
            app:layout_constraintTop_toBottomOf="@id/tv_content_number_title"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tv_type_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/view_line3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="@dimen/vertical_margin_8"
            android:layout_marginStart="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="类型"/>

        <TextView
            android:id="@+id/tv_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="@id/tv_type_title"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="@dimen/horizontal_margin_8"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/c_555"
            android:text="xxx类型"/>
